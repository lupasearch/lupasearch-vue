{
  "name": "@getlupa/vue",
  "version": "0.15.13",
  "main": "dist/lupaSearch.mjs",
  "module": "dist/lupaSearch.mjs",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "dev": "vite & yarn css:watch",
    "css:watch": "rollup --config build/rollupContainerDev.config.js --bundleConfigAsCjs --watch",
    "build": "yarn build-only && yarn css && yarn css:container && yarn css:chat",
    "css": "rollup --config build/lupaStyle.config.js --bundleConfigAsCjs",
    "css:container": "rollup --config build/lupaContainerStyle.config.js --bundleConfigAsCjs",
    "css:chat": "rollup --config build/lupaChatStyle.config.js --bundleConfigAsCjs",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "test:unit": "vitest",
    "test:unit:ci": "vitest run"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@getlupa/client-sdk": "^1.5.4",
    "@pinia/testing": "^0.1.2",
    "@rollup/plugin-babel": "5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-typescript": "^8.3.0",
    "@rushstack/eslint-patch": "^1.2.0",
    "@testing-library/vue": "^7.0.0",
    "@tsconfig/node18": "^2.0.1",
    "@types/jest": "^29.5.2",
    "@types/jsdom": "^21.1.1",
    "@types/lodash": "^4.14.195",
    "@types/node": "^18.16.17",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.4.0",
    "@vue/tsconfig": "^0.4.0",
    "@vueform/slider": "^2.1.7",
    "eslint": "^8.39.0",
    "eslint-plugin-vue": "^9.11.0",
    "happy-dom": "^9.20.3",
    "jest-junit": "^16.0.0",
    "jsdom": "^22.1.0",
    "lodash": "^4.17.21",
    "npm-run-all": "^4.1.5",
    "pinia": "^2.1.3",
    "prettier": "^2.8.8",
    "rollup-plugin-filesize": "^9.1.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-scss": "3",
    "sass": "^1.63.4",
    "start-server-and-test": "^2.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "~5.0.4",
    "vite": "^4.3.9",
    "vite-aliases": "^0.11.2",
    "vite-plugin-dts": "^2.3.0",
    "vitest": "^0.32.2",
    "vue": "^3.3.4",
    "vue-tsc": "^1.6.5",
    "vue3-carousel": "^0.3.1"
  },
  "dependencies": {
    "mustache": "^4.2.0",
    "sanitize-html": "^2.13.0"
  }
